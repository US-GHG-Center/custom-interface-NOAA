{
  "name": "custom-interface-noaa",
  "version": "0.0.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/main.js",
  "source": "src/lib/index.ts",
  "module": "dist/module.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "targets": {
    "noaa-app": {
      "source": "public/index.html",
      "context": "browser"
    }
  },
  "homepage": ".",
  "scripts": {
    "build": "dotenv -- node ./run-parcel.js build build",
    "serve": "dotenv -- node ./run-parcel.js serve",
    "start": "parcel",
    "build-lib": "parcel build ./src/lib/index.ts --dist-dir dist",
    "clean": "rm -rf node_modules dist build yarn.lock .parcel-cache && yarn cache clean",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fix-conflicts": "prettier --write . && eslint . --fix"
  },
  "dependencies": {
    "@date-io/moment": "^3.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.1.6",
    "@mui/x-date-pickers": "^7.22.0",
    "@swc/helpers": "^0.5.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@turf/turf": "^7.1.0",
    "chart.js": "^4.4.3",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-zoom": "^2.0.1",
    "d3": "^7.9.0",
    "luxon": "^3.5.0",
    "mapbox-gl": "^3.7.0",
    "mapboxgl-timeline": "^1.0.2",
    "moment": "^2.30.1",
    "react-loader-spinner": "^6.1.6",
    "react-resizable-panels": "^2.1.1",
    "react-router-dom": "^6.27.0",
    "react-tooltip": "^5.28.0",
    "styled-components": "^6.1.13",
    "typescript": "^4.4.2",
    "uuid": "^11.0.3",
    "web-vitals": "^2.0.1"
  },
  "peerDependencies": {
    "@mui/material": "^6.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@mui/material": "^6.1.6",
    "@parcel/config-default": "^2.15.2",
    "@parcel/packager-raw-url": "^2.15.2",
    "@parcel/packager-ts": "^2.15.2",
    "@parcel/transformer-typescript-types": "^2.15.2",
    "@parcel/transformer-webmanifest": "^2.15.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "dotenv-cli": "^8.0.0",
    "parcel": "^2.15.2",
    "parcel-resolver-tspaths": "^0.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^4.4.2"
  }
}
